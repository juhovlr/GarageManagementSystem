<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Vehicle</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(REGEX( Vehicle_number_plate__c , &quot;[A-Z]{2}[0-9]{2}[A-Z]{2}[0-9]{4}&quot;))</errorConditionFormula>
    <errorDisplayField>Vehicle_number_plate__c</errorDisplayField>
    <errorMessage>Please enter valid number</errorMessage>
</ValidationRule>
